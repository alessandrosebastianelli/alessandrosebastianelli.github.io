@INPROCEEDINGS{8385270, author= {S. L. {Ullo} and C. V. {Angelino} and L. {Cicala} and N. {Fiscante} and P. {Addabbo} and M. P. {Del Rosso} and A. {Sebastianelli}}, booktitle= {2018 IEEE International Conference on Environmental Engineering (EE)}, title= {SAR interferometry with open Sentinel-1 data for environmental measurements: The case of Ischia earthquake}, year= {2018}, volume= {}, number= {}, pages= {1-8}, abstract= {This study rely on open access data as well as open software, both provided by the European Space Agency (ESA) under the Copernicus program. In particular, data from Sentinel-1 radar satellite mission and SNAP software have been used. The availability of such data and software is relevant for public institutions that can produce valuable information at almost no charge. Main aim of the authors has been to produce a review paper, a kind of tutorial, where main characteristics of Sentinel-1 data and SNAP processing tools are presented and discussed. It's important to underline that SNAP is not an easy tool and needs a deep knowledge of electromagnetic aspects to result into reliable results. Together with Sentinel-1 data, interferometry techniques have been employed to get from phase difference information a measure of ground displacements. The Ischia island has been chosen as Case Study because it was affected by a severe earthquake in August 2017. In the days following the earthquake, the area was subject to many topographic surveys. Some of them, carried out by CNR-IREA researchers [1], working in collaboration with INGV, have used DInSAR technique applied to Sentinel-1 and COSMO-SkyMed images, showing a lowering of the soil around the epicenter up to 4 cm [2], [3]. These data have been used in our processing for comparison and validation as explained later in the paper.}, keywords= {geophysical image processing;radar imaging;radar interferometry;remote sensing by radar;spaceborne radar;synthetic aperture radar;SAR interferometry;open Sentinel-1 data;environmental measurements;Ischia earthquake;open access data;open software;European Space Agency;Sentinel-1 radar satellite mission;SNAP software;SNAP processing tools;case study;AD 2017 08;Copernicus program;interferometry techniques;DInSAR technique;Sentinel-1 image;COSMO-SkyMed image;ground displacements;Ischia island;topographic survey;CND-IREA researchers;size 4.0 cm;Synthetic aperture radar;Earthquakes;Interferometry;Satellites;Coherence;Orbits;Distributed databases;Copernicus mission;open access data;Sentinel-1;SNAP;earthquake damage assessment;remote sensing;unwrapping;SNAPHU;Digital Elevation Models (DEMs);tutorial;DInSAR;interferograms;displacements calculation}, doi= {10.1109/EE1.2018.8385270}, ISSN= {}, month= {March}}

